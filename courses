
public class courses {
	private String code,name,creditHours,Section,time,roomNumber,instructorName,numOfRegisteredStudents,currentRegisteredStudent,maxNumStudentForRegister,startDate,endDate;
	
	public courses() {
		this.code = "";
		this.name = "";
		this.creditHours = "";
		this.Section = "";
		this.time = "";
		this.roomNumber = "";
		this.instructorName = "";
		this.numOfRegisteredStudents = "";
		this.currentRegisteredStudent = "";
		this.maxNumStudentForRegister = "";
		this.startDate = "";
		this.endDate = "";
	}
	
	public courses(String code,String name,String creditHours,String Section,String time,String roomNumber,String instructorName,String numOf,String currentRS,String maxNSFR,String startDate,String endDate) {
		this.code = code;
		this.name = name;
		this.creditHours = creditHours;
		this.Section = Section;
		this.time = time;
		this.roomNumber = roomNumber;
		this.instructorName = instructorName;
		this.numOfRegisteredStudents = numOf;
		this.currentRegisteredStudent = currentRS;
		this.maxNumStudentForRegister = maxNSFR;
		this.startDate = startDate;
		this.endDate = endDate;
	}
	
	public String toString() {
		return this.code+" "+this.name+" "+this.creditHours+" "+this.Section+" "+ this.time+" "+this.roomNumber+" "+this.instructorName+" "+this.numOfRegisteredStudents+" "+this.currentRegisteredStudent+" "+this.maxNumStudentForRegister+" "+this.startDate+" "+this.endDate;
	}	
	public void display() {
		System.out.print("Code: " + this.code + 
						 "\nCourse Name: "+ this.name +
						 "\nSection: "+this.Section+ 
						 "\nTime: "+ this.time+
						 "\nRoom number: "+ this.roomNumber+
						 "\nInstructor Name: "+this.instructorName+
						 "\nNumber of registered student: "+this.numOfRegisteredStudents+
						 "\nCurrent registered student: "+ this.currentRegisteredStudent+
						 "\nMax number of student allowed to register: "+ this.maxNumStudentForRegister+
						 "\nStart Date: "+ this.startDate+ 
						 "\nEnd Date: "+ this.endDate);
	}
	public void setCode(String code) {
		this.code = code;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setCreditHours(String creditHours) {
		this.creditHours = creditHours;
	}
	public void setSection(String section) {
		this.Section = section;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public void setRoomNumber(String roomNumber) {
		this.roomNumber = roomNumber;
	}
	public void setInstructorName(String instructorName) {
		this.instructorName = instructorName;
	}
	public void setNumOfRegisteredStudents(String numOfRegisteredStudents) {
		this.numOfRegisteredStudents = numOfRegisteredStudents;
	}
	public void setCurrentRegisteredStudent(String currentRegisteredStudent) {
		this.currentRegisteredStudent = currentRegisteredStudent;
	}
	public void setMaxNumStudentForRegister(String maxNumStudentForRegister) {
		this.maxNumStudentForRegister = maxNumStudentForRegister;
	}
	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}
	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}
	
	
	public String getCode() {
		return this.code;
	}
	public String getName() {
		return this.name;
	}
	public String getCreditHours() {
		return this.creditHours;
	}
	public String getSection() {
		return this.Section;
	}
	public String getTime() {
		return this.time;
	}
	public String getRoomNumber() {
		return this.roomNumber;
	}
	public String getInstructorName() {
		return this.instructorName;
	}
	public String getNumOfRegisteredStudents() {
		return this.numOfRegisteredStudents;
	}
	public String getCurrentRegisteredStudent() {
		return this.currentRegisteredStudent;
	}
	public String getMaxNumStudentForRegister() {
		return this.maxNumStudentForRegister;
	}
	public String getStartDate() {
		return this.startDate;
	}
	public String setEndDate() {
		return this.endDate;
	}
	
	
}
